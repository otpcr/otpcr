#!/usr/bin/env python3
# This file is placed in the Public Domain.
# pylint: disable=R,C0413


"Office of the Prosecutor Communication 117 of the year 2019."


import getpass
import os
import readline
import sys


from otpcr.command import command, parse
from otpcr.modules import face
from otpcr.object  import Default
from otpcr.runtime import Client, Event, errors


class CLI(Client):

    "CLI"

    def raw(self, txt):
        "print text."
        print(txt)


def main():
    "main"
    cfg = Default()
    parse(cfg, " ".join(sys.argv[1:]))
    cli = CLI()
    evt = Event()
    evt.orig = repr(cli)
    evt.txt = cfg.txt
    command(cli, evt)


if __name__ == "__main__":
    main()
    errors(print)
